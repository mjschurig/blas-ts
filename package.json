{
  "name": "blas-ts",
  "version": "1.1.0",
  "description": "Pure TypeScript implementation of BLAS (Basic Linear Algebra Subprograms)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' 'tests/**/*.ts' --fix",
    "prepare": "husky"
  },
  "keywords": [
    "blas",
    "linear-algebra",
    "typescript",
    "mathematics",
    "numerical-computing"
  ],
  "author": "mjschurig",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mjschurig/blas-ts.git"
  },
  "bugs": {
    "url": "https://github.com/mjschurig/blas-ts/issues"
  },
  "homepage": "https://github.com/mjschurig/blas-ts#readme",
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.6.2",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.37.0",
    "eslint-plugin-jest": "^29.0.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.11.0"
}
